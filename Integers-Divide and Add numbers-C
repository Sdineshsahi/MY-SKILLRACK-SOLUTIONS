#include<stdio.h>
#include<stdlib.h>

int main()
{
    int n,k;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    scanf("%d",&k);
    for(int i=0;i<k;i++){
        for(int j=k;j<n;j++){
            arr[j]=arr[j]+(arr[j]%arr[i]);
        }
    }
    for(int i=0;i<n;i++){
        printf("%d ",arr[i]);
    }
}
