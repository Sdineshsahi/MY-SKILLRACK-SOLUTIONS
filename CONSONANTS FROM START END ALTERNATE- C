#include<stdio.h>
#include<stdlib.h>
int isv(char c){
    c=tolower(c);
    if(c=='a'||c=='e'||c=='o'||c=='i'||c=='u'){
        return 1;
    }
    else{
        return 0;
    }
}
int main()
{
    char s[1001];
    scanf("%s",s);
    char *res=(char*)malloc(1001*sizeof(char));
    int k=0;
    for(int i=0;i<strlen(s);i++){
        if(isv(s[i])==0){
            res[k++]=s[i];
        }
    }
    for(int i=0;i<k/2;i++){
        printf("%c",res[i]);
        printf("%c",res[k-i-1]);
    }
    if(strlen(res)%2!=0){
        printf("%c",res[k/2]);
    }
    printf("\nCoded by Dinesh");
}
