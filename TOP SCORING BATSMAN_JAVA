import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		sc.nextLine();
		String res="";
		int t=0;
		for(int i=0;i<n;i++){
		    String s=sc.nextLine();
		    String[]k=s.split(",");
		    String name=k[0];
		    int r=Integer.parseInt(k[1]);
		    if(r>t){
		        t=r;
		        res=name;
		    }
		}
		System.out.print(res);

	}
}
